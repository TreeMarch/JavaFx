<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.cell.PropertyValueFactory?>

<GridPane alignment="CENTER" hgap="10.0" vgap="10.0"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="org.example.managestudents.controller.DemoTableDemo">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <Label style="-fx-font: NORMAL 20 Tahoma;" text="List Student"
           GridPane.columnIndex="0" GridPane.rowIndex="0">
    </Label>
    <TableView fx:id="tableView" GridPane.columnIndex="0"
               GridPane.rowIndex="1">
        <columns>
            <TableColumn text="Code" prefWidth="100">
                <cellValueFactory><PropertyValueFactory property="code" /></cellValueFactory>
            </TableColumn>
            <TableColumn text="Full Name" prefWidth="200">
                <cellValueFactory><PropertyValueFactory property="fullName" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="Phone" prefWidth="200">
                <cellValueFactory><PropertyValueFactory property="phoneumber" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="Email" prefWidth="200">
                <cellValueFactory><PropertyValueFactory property="email" />
                </cellValueFactory>
            </TableColumn>
        </columns>
    </TableView>
</GridPane>